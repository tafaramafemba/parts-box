<%= form_with(model: [:admin, commission_fee], local: true) do |form| %>
  <div class="form-group">
    <%= form.label :percentage %>
    <%= form.number_field :percentage, step: 0.01, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.submit class: 'btn btn-primary' %>
  </div>
<% end %>