<%= form_with(model: [:admin, category_shipping_fee], local: true) do |form| %>
  <div class="form-group">
    <%= form.label :category %>
    <%= form.text_field :category, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.label :fee %>
    <%= form.number_field :fee, step: 0.01, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.submit class: 'btn btn-primary' %>
  </div>
<% end %>